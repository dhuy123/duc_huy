package bai3_2;

import java.util.Scanner;

public class tamGiac extends diem {
	private diem a,b,c;
	public tamGiac(int x, int y) {
		super(x,y);
		diem a,b,c;
		}
	public void outDiem() {
		System.out.println("nhap điểm a");
		a.nhapDiem();
	}
	public static void main(String[] args) {
		tamGiac a=new tamGiac(0,0);
		a.outDiem();
		tamGiac b=new tamGiac(0,0);
		b.nhapDiem();
		tamGiac c=new tamGiac(0,0);
		c.nhapDiem();
		a.inDiem();
		b.inDiem();
		c.inDiem();
		
	}
	

}
