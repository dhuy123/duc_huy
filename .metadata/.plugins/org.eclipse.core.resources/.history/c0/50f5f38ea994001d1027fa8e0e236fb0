package baitaplan2;

import java.util.Scanner;

public class bai5 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("nhập vào 2 số nguyên a b:");
	int a=sc.nextInt();
	int b=sc.nextInt();
	for (int i=0;i>=b&&i>=a;i++) {{
		if(a%400==0||b%4==0);
	     System.out.println("năm nhuận là :"+i);}
	    if(a%4!=0||b%4!=0) {
	    	 System.out.println("không có năm nhuận");}
	
	
	
	
}
}
}
