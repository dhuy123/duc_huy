package bai3_2;

import java.util.Scanner;

public class main {
public static void main(String[] args) {
	Scanner sc=new Scanner (System.in);
	int lc;
	System.out.println("\slựa chọn:\s");
	lc=sc.nextInt();
	switch (lc){
		case 1:
			diem d=new diem(0,0);
			d.nhapDiem();
			d.inDiem();
			break;
		case 2:
			tamGiac t=new tamGiac(0,0);
			t.nhapTG();
			t.inTamGiac();
			t.chuvi();
			t.dientich();
			break;
			
		case 3:
		duongtron c=new duongtron();
		c.nhapdt();
		c.indt();
		c.chuvi();
		c.dientich();
		
		}
	
}
}
